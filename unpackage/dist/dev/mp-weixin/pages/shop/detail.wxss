#movementDetail {
  height: 100%;
}
#movementDetail #title {
    text-align: center;
    border-bottom: 1px solid #bfbfbf;
    line-height: 50px;
    font-size: 17px;
    background: #f4f4f4;
    height: 50px;
    position: relative;
}
#movementDetail #title .return {
      position: absolute;
      left: 17px;
      width: 16px;
      height: 16px;
      top: 17px;
      -webkit-transform: rotateZ(-45deg);
          -ms-transform: rotate(-45deg);
              transform: rotateZ(-45deg);
